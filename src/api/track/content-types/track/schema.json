{
  "kind": "collectionType",
  "collectionName": "tracks",
  "info": {
    "singularName": "track",
    "pluralName": "tracks",
    "displayName": "Track"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "duration": {
      "type": "string"
    },
    "preview": {
      "allowedTypes": [
        "audios"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "file": {
      "allowedTypes": [
        "audios"
      ],
      "type": "media",
      "multiple": false,
      "required": false
    },
    "albums": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::album.album",
      "inversedBy": "tracks"
    },
    "genres": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::genre.genre",
      "inversedBy": "tracks"
    }
  }
}
